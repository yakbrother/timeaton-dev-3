{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@content/*": ["../content/*"],
      "@layouts/*": ["layouts/*"],
      "@components/*": ["components/*"],
      "@data/*": ["lib/data/*"],
      "@helpers/*": ["lib/helpers/*"],
      "@styles/*": ["styles/*"]
    },
    "jsx": "preserve"
  }
}
